{
  "imports": {
    "@concepts/": "./src/concepts/",
    "@concepts": "./src/concepts/concepts.ts",
    "@hono/hono": "jsr:@hono/hono@^4.10.6",
    "@test-concepts": "./src/concepts/test_concepts.ts",
    "@utils/": "./src/utils/",
    "@engine/": "./src/engine/",
    "@engine": "./src/engine/mod.ts",
    "@syncs/": "./src/syncs/",
    "@syncs": "./src/syncs/syncs.ts"
  },
  "tasks": {
    "start": "deno run --import-map=deno.json --allow-net --allow-write --allow-read --allow-sys --allow-env src/main.ts",
    "concepts": "deno run --import-map=deno.json --allow-net --allow-read --allow-sys --allow-env src/concept_server.ts --port 8000 --baseUrl /api",
    "import": "deno run --import-map=deno.json --allow-read --allow-write --allow-env src/utils/generate_imports.ts",
    "build": "deno run --import-map=deno.json --allow-read --allow-write src/utils/generate_imports.ts"
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-import-prefix",
        "no-unversioned-import"
      ]
    }
  }
}
